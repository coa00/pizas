{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true}]
    },
    "globals": {
      "process": true
    }
}